{
    "name": "m3uzz/onion-srv-skeleton",
    "description": "Skeleton Application for Onion Srv",
    "homepage": "http://onion.m3uzz.com/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Humberto S Lourenco",
            "email": "betto@m3uzz.com"
        }
    ], 
   "keywords": [
        "framework",
        "m3uzz",
        "onion",
	"srv"
    ],
    "type": "package",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "m3uzz/onion-srv-skeleton",
                "version": "master",
                "dist": {
                    "url": "https://github.com/m3uzz/onion-srv-skeleton/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/m3uzz/onion-srv-skeleton.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4",
	"m3uzz/onionsrv": "master"
    },
    "require-dev": {
    	"m3uzz/oniontool": "master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
    ],
   "scripts": {
        "post-update-cmd": [ ],
        "post-package-install": [ ],
        "post-install-cmd": [ ],
        "post-autoload-dump": [ ],
        "post-create-project-cmd": [
        	"cd bin",
        	"ln -s ../vendor/m3uzz/oniontool/bin/cmstool.php cmstool",
        	"ln -s ../vendor/m3uzz/oniontool/bin/srvtool.php srvtool",
        	"ls -al"
        ]
    }

}
